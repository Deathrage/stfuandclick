{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "object-curly-spacing":"off",
        "no-tabs":"off",
        "indent": ["error", "tab"],
        "require-jsdoc": "off",
        "prefer-const": "off",
        "padded-blocks": "off",
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["error", "always"],
        "space-before-function-paren": "off",
        "no-dupe-class-members": "off",
        "no-useless-constructor": "off"
    }
}